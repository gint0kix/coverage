{
  "name": "coverage",
  "version": "1.0.0",
  "description": "Coverage Finder, using the MERN stack",
  "main": "server/server.js",
  "scripts": {
    "start": "cd server && node server.js",
    "server": "nodemon server.js",
    "client": "cd client && npm run start",
    "app": "concurrently --raw --kill-others \"npm run start\" \"npm run client\""
  },
  "author": "Chris Guevarra",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "config": "^3.3.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
